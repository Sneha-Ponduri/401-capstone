<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <style>
        body {
            margin: 0;
            padding: 0;
            font-family: 'Roboto', sans-serif;
            background:url(data:image/jpeg;base64,);
            background-size: cover;
            background-repeat: no-repeat;
            height: 100vh;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .container {
            background: black;
            border-radius: 10px;
            box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
            width: 700px;
            padding: 30px;
            box-sizing: border-box;
            text-align: center;
            color:red;
        }
        .container h2 {
            margin-bottom: 20px;
            font-size: 24px;
            font-weight: bold;
        }
        .container p {
            font-size: 16px;
            color: white;
        }
        .container form {
            margin-bottom: 20px;
        }
        .container input[type="text"] {
            width: calc(100% - 22px);
            padding: 10px;
            margin-bottom: 10px;
            border: 1px solid #ccc;
            border-radius: 5px;
        }
        .container input[type="submit"] {
            padding: 10px 20px;
            background: black;
            border: none;
            border-radius: 5px;
            color: red;
            cursor: pointer;
        }
        .container .recipe-results {
            margin-top: 20px;
        }
        .container .recipe {
            margin-bottom: 10px;
            text-align: left;
        }
        .container .recipe h4 {
            margin: 0;
        }
        .container .recipe p {
            margin: 5px 0;
        }
    </style>
</head>
<body>
    <div class="container">
        <h2>Welcome to your Dashboard, <%= username %>!</h2>
        <p>Login successful!</p>
        <form id="recipe-form">
            <input type="text" id="recipe-name" placeholder="Enter recipe name" required>
            <input type="submit" value="Get Recipe">
        </form>
        <div class="recipe-results" id="recipe-results"></div>
    </div>
    <script>
        document.getElementById('recipe-form').addEventListener('submit', async function(event) {
            event.preventDefault();
            const recipeName = document.getElementById('recipe-name').value;
            const apiKey = '<%= apiKey %>';
            const appId = '<%= appId %>';
            const response = await fetch(`https://api.edamam.com/search?q=${recipeName}&app_id=${appId}&app_key=${apiKey}`);
            const data = await response.json();
            const resultsContainer = document.getElementById('recipe-results');
            resultsContainer.innerHTML = '';
            if (data.hits.length > 0) {
                data.hits.forEach(hit => {
                    const recipeDiv = document.createElement('div');
                    recipeDiv.classList.add('recipe');
                    recipeDiv.innerHTML = `
                        <h4>${hit.recipe.label}</h4>
                        <p>Calories: ${Math.round(hit.recipe.calories)}</p>
                        <p><a href="${hit.recipe.url}" target="_blank">View Recipe</a></p>
                    `;
                    resultsContainer.appendChild(recipeDiv);
                });
            } else {
                resultsContainer.innerHTML = '<p>No recipes found.</p>';
            }
        });
    </script>
</body>
</html>
